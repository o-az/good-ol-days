{
  "name": "good-ol-days",
  "type": "module",
  "private": true,
  "module": "./dist/main.mjs",
  "repository": "github:o-az/good-ol-days",
  "imports": {
    "#*": "./src/*",
    "#package.json": "./package.json"
  },
  "scripts": {
    "start": "node --run webpack -- serve --mode='production' --config='webpack.config.ts'",
    "build": "node --run webpack -- build --mode='production' --config='webpack.config.ts'",
    "dev": "node --run webpack -- serve --mode='development' --config='webpack.config.ts'",
    "webpack": "NODE_OPTIONS='--import @oxc-node/core/register' webpack",
    "check": "biome check . --write --unsafe",
    "check:types": "tsgo --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "jquery": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@oxc-node/core": "^0.0.35",
    "@types/jquery": "^3.5.33",
    "@types/node": "^25.0.9",
    "@types/webpack": "^5.28.5",
    "@typescript/native-preview": "^7.0.0-dev.20260116.1",
    "html-webpack-plugin": "^5.6.5",
    "ts-loader": "^9.5.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "packageManager": "npm@11.7.0",
  "license": "MIT"
}
